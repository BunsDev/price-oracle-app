{
  "name": "price-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "format:check": "npm run prettier && npm run lint",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "@floating-ui/react": "0.14.0",
    "@price-oracle/interfaces": "0.0.0-a375d387",
    "@rainbow-me/rainbowkit": "0.7.4",
    "@reduxjs/toolkit": "1.8.6",
    "@uniswap/v3-sdk": "3.9.0",
    "bignumber.js": "9.1.0",
    "ethers": "5.7.2",
    "ethers-multicall": "0.2.3",
    "jsbi": "3.2.5",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.2",
    "react-transition-group": "4.4.5",
    "redux-localstorage-simple": "2.5.1",
    "redux-logger": "3.0.6",
    "styled-components": "5.3.6",
    "ts-node": "10.9.1",
    "wagmi": "0.7.9"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@defi-wonderland/crypto-husky-checks": "1.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "4.14.188",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/react-transition-group": "4.4.5",
    "@types/redux-logger": "3.0.9",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "jsdom": "20.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.25.8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md,html}": "prettier --write --ignore-unknown"
  }
}
